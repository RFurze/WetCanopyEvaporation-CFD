FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      s;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    bottom
    {
        type           fixedValue;
        value          uniform 0;
    }

    top
    {
        type           zeroGradient;
    }

    "(inlet|outlet|left|right)"
    {
        type            cyclic;
    }
}


